
<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle">
        <label>Post</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
        <label>Date</label>
        <input id="dt" class=" form-control" type="date" name="postDate" value=toLocalDateString()>
      </div>



      <button class="col-sm-2 btn btn-primary" type="submit" name="button">Publish</button>

    </form>

<%- include("partials/footer"); -%>
